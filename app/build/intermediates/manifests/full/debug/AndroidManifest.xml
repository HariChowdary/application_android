<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="charts.onion.it.tutor"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme" >
        <activity android:name="charts.onion.it.tutorgirish1.Splashscreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="charts.onion.it.tutorgirish1.Login"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity
            android:name="charts.onion.it.tutorgirish1.Register"
            android:windowSoftInputMode="stateHidden|adjustPan" />
        <activity android:name="charts.onion.it.tutorgirish1.MainActivity" >
        </activity>
        <activity android:name="charts.onion.it.tutorgirish1.VideoActivity" />
    </application>

</manifest>